{
  "atom": "1.13.0-beta8",
  "pigments": "0.38.0",
  "platform": "darwin",
  "config": {
    "autocompleteSuggestionsFromValue": true,
    "extendAutocompleteToColorValue": true,
    "extendAutocompleteToVariables": true,
    "extendedSearchNames": [
      "**/*.css",
      "**/*.scss",
      "**/*.less"
    ],
    "mergeColorDuplicates": true,
    "sortPaletteColors": "by color",
    "traverseIntoSymlinkDirectories": true,
    "sourceNames": [
      "**/*.css",
      "**/*.styl",
      "**/*.stylus",
      "**/*.less",
      "**/*.sass",
      "**/*.scss"
    ],
    "ignoredNames": [
      "vendor/*",
      "node_modules/*",
      "spec/*",
      "test/*"
    ],
    "ignoredBufferNames": [],
    "supportedFiletypes": [
      "*"
    ],
    "filetypesForColorWords": [
      "css",
      "less",
      "styl",
      "stylus",
      "sass",
      "scss"
    ],
    "ignoredScopes": [],
    "autocompleteScopes": [
      ".source.css",
      ".source.css.less",
      ".source.sass",
      ".source.css.scss",
      ".source.stylus"
    ],
    "markerType": "native-background",
    "sassShadeAndTintImplementation": "compass",
    "groupPaletteColors": "none",
    "delayBeforeScan": 500,
    "ignoreVcsIgnoredPaths": true,
    "notifyReloads": true,
    "dismissableReloadNotifications": true
  },
  "project": {
    "config": {},
    "variables": {
      "colors": 0,
      "total": 0
    }
  }
}
