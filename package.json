  "configSchema": {
    "tabSizing": {
      "title": "Full-Width Tab Sizing",
      "description": "In full width mode, tabs will fill the whole tab bar.",
      "type": "boolean",
      "default": true
    },
    "fixedTabBar": {
      "title": "Fixed Tab Bar",
      "description": "Allow tabs to spread amongst the whole screen.",
      "type": "boolean",
      "default": true
    },
    "fixedProjectRoot": {
      "title": "Fixed Project Root",
      "description": "Project root folder visible even when scrolled down.",
      "type": "boolean",
      "default": true
    }
  },
